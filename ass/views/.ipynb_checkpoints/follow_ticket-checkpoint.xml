<odoo>
    <data>
        <record model="ir.ui.view" id="sav_autre">
      <field name="name">ctr.list</field>
      <field name="model">helpdesk.ticket</field>
      <field name="inherit_id" ref="helpdesk.helpdesk_ticket_view_form" />
      <field name="arch" type="xml">
          <field name="description" position="before">
              <notebook>
                  <page string="Autre informations">
                      <group>
                          <separator colspan="2" string="Informations Partenaire" />
                          <field name="cust"/>
                          <field name="phone"/>
                      </group>
                      <group>
                          <separator colspan="2" string="Informations Demandeur" />
                          <field name="applicant_name" />
                          <field name="applicant_phone" />                        
                      </group>
                  </page>
                  <page string="Suivi">
                  </page>
              </notebook>
          </field>
      </field>
    </record>
    </data>
</odoo>